config_version=5

[application]

config/name="bounce"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/icon.png"
config/icon="res://assets/icon.svg"
boot_splash/minimum_display_time=2000

[audio]

buses/default_bus_layout="res://assets/audio/default_bus_layout.tres"

[autoload]

TouchInput="*res://players/touchInput.gd"
ScoreKeeper="*res://players/scoreKeeper.gd"

[display]

window/size/viewport_width=675
window/size/viewport_height=1575
window/size/window_width_override=600
window/size/window_height_override=600
window/handheld/orientation=6
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/custom="res://assets/theme/pupperTheme.tres"
theme/custom_font="res://assets/theme/Open_Sans/OpenSans-VariableFont_wdth,wght.ttf"

